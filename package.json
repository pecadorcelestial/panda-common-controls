{
  "name": "panda-common-controls",
  "version": "1.1.18",
  "description": "Common controls library for websites.",
  "main": "index",
  "typings": "index",
  "scripts": {
    "clean": "bash scripts/clean-lib.sh",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib --ignore **/*.test.js,app.js,client.js",
    "prepublishOnly": "npm run test && npm run build",
    "prestart": "webpack -p",
    "start": "set NODE_ENV=production && babel-node --presets @babel/env,@babel/react --plugins @babel/plugin-syntax-dynamic-import,dynamic-import-node,transform-class-properties server.js",
    "start:dev": "set NODE_ENV=development && babel-node --presets @babel/env,@babel/react --plugins @babel/plugin-syntax-dynamic-import,dynamic-import-node,transform-class-properties server.js",
    "test": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Francisco Rodr√≠guez",
  "license": "ISC",
  "keywords": [
    "components",
    "controls",
    "library",
    "panda",
    "react",
    "react-component",
    "animation",
    "badge",
    "button",
    "calendar",
    "card",
    "checkbox",
    "dropdownlist",
    "icons",
    "radiobutton",
    "textbox",
    "toast-notification",
    "toast",
    "notification"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "3.4.10",
    "@storybook/addon-links": "3.4.10",
    "@storybook/addons": "3.4.10",
    "@storybook/react": "3.4.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^0.1.19",
    "cookie-parser": "^1.4.3",
    "coveralls": "^3.0.2",
    "ejs": "^2.6.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jest-enzyme": "^6.0.2",
    "jest-styled-components": "^6.2.0",
    "jsdom": "^12.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "styled-components": "^3.3.3",
    "styled-theming": "^2.2.0",
    "webfontloader": "^1.6.28",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3"
  },
  "peerDependencies": {
    "styled-components": "^3.3.3",
    "styled-theming": "^2.2.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/**.{js,jsx}",
      "!bin/**",
      "!node_modules/**",
      "!src/**/*.storybook.jsx",
      "!src/app.js",
      "!src/client.js",
      "!config.js",
      "!*.config.js"
    ],
    "roots": [
      "<rootDir>"
    ],
    "setupTestFrameworkScriptFile": "./jest.config.js",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/bin/",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ]
  }
}
