{
  "name": "panda-common-controls",
  "version": "1.0.12",
  "description": "Common controls library for websites.",
  "main": "index.js",
  "scripts": {
    "clean": "bash scripts/clean-lib.sh",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib --ignore **/*.test.js,app.js,client.js",
    "prepublishOnly": "npm run test && npm run build",
    "test": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Francisco Rodr√≠guez",
  "license": "ISC",
  "dependencies": {
    "express": "4.16.3",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "styled-components": "3.3.3",
    "styled-theming": "2.2.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.8",
    "@storybook/addon-links": "3.4.8",
    "@storybook/addons": "3.4.8",
    "@storybook/react": "3.4.8",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "clean-webpack-plugin": "0.1.19",
    "coveralls": "3.0.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "23.4.0",
    "jest-enzyme": "6.0.2",
    "jest-styled-components": "5.0.1",
    "jsdom": "11.11.0",
    "react-test-renderer": "16.4.1",
    "webpack": "4.16.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/**.{js,jsx}",
      "!bin/**",
      "!node_modules/**",
      "!src/**/*.storybook.jsx",
      "!src/app.js",
      "!src/client.js",
      "!config.js",
      "!*.config.js"
    ],
    "roots": [
      "<rootDir>"
    ],
    "setupTestFrameworkScriptFile": "./jest.config.js",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/bin/",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ]
  }
}
